{
  "compilerOptions": {
    "target": "es2022",
    "module": "esnext",
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "skipLibCheck": true,
    "experimentalDecorators": true,
    "baseUrl": ".",
    "paths": {
      "@app-speed/aws-deploy": ["packages/aws-deploy/src/index.ts"],
      "@app-speed/audit-queue/*": ["packages/audit-queue/src/lib/*"],
      "@app-speed/audit-store/*": ["packages/audit-store/src/lib/*"],
      "@app-speed/cli-interfaces": ["packages/cli-interfaces/src/index.ts"],
      "@app-speed/cli-middleware": ["packages/cli-middleware/src/index.ts"],
      "@app-speed/data-access": ["packages/data-access/public-api.ts"],
      "@app-speed/environments": ["packages/environments/src/index.ts"],
      "@app-speed/feature/audit-builder": ["packages/feature/audit-builder/src/index.ts"],
      "@app-speed/feature/audit-viewer": ["packages/feature/audit-viewer/src/index.ts"],
      "@app-speed/shared": ["packages/shared/public-api.ts"],
      "@app-speed/ui/loading-spinner": ["packages/ui/loading-spinner/public-api.ts"],
      "@app-speed/ui/radial-chart": ["packages/ui/radial-chart/public-api.ts"],
      "@app-speed/ui/scroll-container": ["packages/ui/scroll-container/public-api.ts"],
      "@app-speed/ui/shell": ["packages/ui/shell/public-api.ts"],
      "@app-speed/ui/status-badge": ["packages/ui/status-badge/public-api.ts"],
      "@app-speed/user-flow-replay": ["packages/user-flow-replay/public-api.ts"]
    }
  }
}
