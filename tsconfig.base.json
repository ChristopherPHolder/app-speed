{
  "compilerOptions": {
    "target": "es2022",
    "module": "esnext",
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "skipLibCheck": true,
    "experimentalDecorators": true,
    "baseUrl": ".",
    "paths": {
      "@app-speed/audit-queue/*": ["packages/audit-queue/src/lib/*"],
      "@app-speed/aws-deploy": ["libs/aws-deploy/src/index.ts"],
      "@app-speed/data-access": ["packages/data-access/src/index.ts"],
      "@app-speed/feature/audit-builder": ["packages/feature/audit-builder/src/index.ts"],
      "@app-speed/feature/audit-viewer": ["packages/feature/audit-viewer/src/index.ts"],
      "@app-speed/ui/loading-spinner": ["packages/ui/loading-spinner/public-api.ts"],
      "@app-speed/ui/radial-chart": ["packages/ui/radial-chart/public-api.ts"],
      "@app-speed/ui/scroll-container": ["packages/ui/scroll-container/public-api.ts"],
      "@app-speed/ui/shell": ["packages/ui/shell/public-api.ts"],
      "@app-speed/ui/status-badge": ["packages/ui/status-badge/public-api.ts"],
      "@ufo/audit-store/*": ["libs/audit-store/src/lib/*"],
      "@ufo/cli-interfaces": ["libs/cli-interfaces/src/index.ts"],
      "@ufo/cli-middleware": ["libs/cli-middleware/src/index.ts"],
      "@ufo/user-flow-replay": ["libs/user-flow-replay/src/index.ts"],
      "environments": ["libs/environments/src/index.ts"],
      "shared": ["libs/shared/src/index.ts"],
    }
  }
}
