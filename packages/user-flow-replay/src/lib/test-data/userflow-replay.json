{
  "title": "Order Coffee",
  "steps": [
    {
      "type": "setViewport",
      "width": 953,
      "height": 616,
      "deviceScaleFactor": 1,
      "isMobile": false,
      "hasTouch": false,
      "isLandscape": false
    },
    {
      "type": "startTimespan"
    },
    {
      "type": "click",
      "target": "main",
      "selectors": [
        [
          "aria/Mocha"
        ],
        [
          "[data-test=Mocha]"
        ]
      ],
      "offsetY": 0,
      "offsetX": 0
    },
    {
      "type": "click",
      "target": "main",
      "selectors": [
        [
          "aria/Proceed to checkout"
        ],
        [
          "[data-test=checkout]"
        ]
      ],
      "offsetY": 0,
      "offsetX": 0
    },
    {
      "type": "change",
      "value": "chris@holder.dev",
      "selectors": [
        [
          "aria/Name"
        ],
        [
          "#name"
        ]
      ],
      "target": "main"
    },
    {
      "type": "endTimespan"
    },
    {
      "type": "snapshot"
    }
  ]
}
