{
  "name": "angular-rollup",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/angular-rollup/src",
  "projectType": "library",
  "tags": ["nx-plugin"],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/angular-rollup",
        "main": "packages/angular-rollup/src/index.ts",
        "tsConfig": "packages/angular-rollup/tsconfig.lib.json",
        "assets": [
          "packages/angular-rollup/*.md",
          {
            "input": "./packages/angular-rollup/src",
            "glob": "**/!(*.ts)",
            "output": "./src"
          },
          {
            "input": "./packages/angular-rollup/src",
            "glob": "**/*.d.ts",
            "output": "./src"
          },
          {
            "input": "./packages/angular-rollup",
            "glob": "generators.json",
            "output": "."
          },
          {
            "input": "./packages/angular-rollup",
            "glob": "executors.json",
            "output": "."
          }
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/angular-rollup/jest.config.ts"
      }
    }
  }
}
