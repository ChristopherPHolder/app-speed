{
  "name": "is-online",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/is-online/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx-aws/sam:build",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "target": "node",
        "compiler": "tsc",
        "outputPath": "dist/apps/is-online",
        "main": "apps/is-online/src/main.ts",
        "tsConfig": "apps/is-online/tsconfig.app.json",
        "assets": [
          "apps/is-online/src/assets"
        ],
        "template": "apps/is-online/src/template.yaml",
        "sourceMap": true,
        "maxWorkers": 1
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false,
          "fileReplacements": [
            {
              "replace": "apps/is-online/src/environments/environment.ts",
              "with": "apps/is-online/src/environments/environment.prod.ts"
            }
          ]
        }
      }
    },
    "serve": {
      "executor": "@nx-aws/sam:execute",
      "options": {
        "buildTarget": "is-online:build",
        "packageTarget": "is-online:package"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "apps/is-online/**/*.ts"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "apps/is-online/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "package": {
      "executor": "@nx-aws/sam:package",
      "options": {
        "templateFile": "apps/is-online/src/template.yaml",
        "outputTemplateFile": "dist/apps/is-online/serverless-output.yaml",
        "s3_bucket": "ufo-is-online",
        "s3Prefix": "apps/is-online"
      },
      "configurations": {
        "production": {}
      }
    },
    "deploy": {
      "executor": "@nx-aws/sam:deploy",
      "options": {
        "templateFile": "dist/apps/is-online/serverless-output.yaml",
        "s3_bucket": "ufo-is-online",
        "s3Prefix": "apps/is-online",
        "capabilities": [
          "CAPABILITY_IAM",
          "CAPABILITY_AUTO_EXPAND"
        ]
      },
      "configurations": {
        "production": {
          "stackSuffix": "prod"
        }
      }
    }
  },
  "tags": []
}
